{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-spike"
		},
		"AzureBatch1_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'AzureBatch1'"
		},
		"AzureBatchLinkedService_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'AzureBatchLinkedService'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"AzureBlobStorage3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage3'"
		},
		"AzureBlobStorageCSVFiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageCSVFiles'"
		},
		"linkedServiceHDInsight_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedServiceHDInsight'"
		},
		"AzureBlobStorageHDInsight_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageHDInsight'"
		},
		"AzureBatch1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "adfspike"
		},
		"AzureBatch1_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://adfspike.southeastasia.batch.azure.com"
		},
		"AzureBatch1_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "pool1test"
		},
		"AzureBatchLinkedService_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "adfspike"
		},
		"AzureBatchLinkedService_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://adfspike.southeastasia.batch.azure.com"
		},
		"AzureBatchLinkedService_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "adfspikepool"
		},
		"linkedServiceHDInsight_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"newCSVTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/cea60126-7f64-4b7d-9722-717b145cc99f/resourceGroups/adf-spike/providers/Microsoft.Storage/storageAccounts/adfspikestorageaccount"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-spike_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"AzureBlobStorage3_connectionString": {
						"value": "[parameters('AzureBlobStorage3_connectionString')]"
					},
					"AzureBlobStorageCSVFiles_connectionString": {
						"value": "[parameters('AzureBlobStorageCSVFiles_connectionString')]"
					},
					"AzureBlobStorageHDInsight_connectionString": {
						"value": "[parameters('AzureBlobStorageHDInsight_connectionString')]"
					},
					"AzureBatch1_accessKey": {
						"value": "[parameters('AzureBatch1_accessKey')]"
					},
					"AzureBatchLinkedService_accessKey": {
						"value": "[parameters('AzureBatchLinkedService_accessKey')]"
					},
					"linkedServiceHDInsight_password": {
						"value": "[parameters('linkedServiceHDInsight_password')]"
					},
					"AzureBatch1_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_accountName')]"
					},
					"AzureBatch1_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_batchUri')]"
					},
					"AzureBatch1_properties_typeProperties_poolName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_poolName')]"
					},
					"AzureBatchLinkedService_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatchLinkedService_properties_typeProperties_accountName')]"
					},
					"AzureBatchLinkedService_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatchLinkedService_properties_typeProperties_batchUri')]"
					},
					"AzureBatchLinkedService_properties_typeProperties_poolName": {
						"value": "[parameters('AzureBatchLinkedService_properties_typeProperties_poolName')]"
					},
					"linkedServiceHDInsight_properties_typeProperties_userName": {
						"value": "[parameters('linkedServiceHDInsight_properties_typeProperties_userName')]"
					},
					"newCSVTrigger_properties_typeProperties_scope": {
						"value": "[parameters('newCSVTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}